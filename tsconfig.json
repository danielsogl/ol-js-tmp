{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "declaration": true,
    "sourceMap": false,
    "baseUrl": "./",
    "paths": {
      "ol": ["node_modules/ol/src"],
      "ol/*": ["node_modules/ol/src/*"]
    }
  },
  "include": ["./src/**/*", "node_modules/ol/**/*"],
  "typeAcquisition": {
    "exclude": ["ol"]
  }
}
